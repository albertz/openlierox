blender = add
alpha = 100
col_layer = 1
render_layer = 5
ground_friction = 0
bounce_factor = 0
damping = 0.99
light_radius = 5
health = 1

on death()
 remove()

on creation()
 set_alpha_fade( -100, 0)

on timer( 100, max_trigger = 1 )
 set_alpha_fade( 100, 0)

on timer( 200 )
 remove()
